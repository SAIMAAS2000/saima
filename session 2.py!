count=int(input("Enter a dice value"))
if(count==8):
            print("you go up")
else:
            print("continue playing")
if(count==11):
            print("snake bites")
else:
            print("continue playing")
if(count==13):
            print("climbs up the stairs")
else:
            print("continue playing")
